.box{
  background-color: white;
}
.title1{
  display: flex;
  height: 100rpx;
  background-color: rgb(204, 69, 35);
  margin-bottom: 10px;
}
.left{
  position: relative;
  left: 1.5%;
  padding-top: 14px;
  font-weight: 1000;
  color:rgb(253, 176, 61);
}
.right{
  position: absolute;
  right: 5%;
  padding-top: 17px;
  font-weight: 400;
  font-size: small;
  color:rgb(253, 176, 61);
}
.title2{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 18px;
  text-align: center;
  padding-top: 10px;
  padding-bottom: 20rpx;
  font-weight: bolder;
  color: rgb(206, 56, 10);
}
/*分割线*/
.line{
  height: 3rpx;
  background-color: gray;
}
.box2{
  background-color: rgb(206, 56, 10);
}
.subtitle{
  text-align: center;
  font-size: small;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  color:gray;
}
.content{
  white-space: pre-wrap;
}
.bottom{ 
  background-color: rgb(206, 56, 10);
}
.copyright{
  color: white;
  text-align: center;
  font-size: small;
  padding-bottom: 10rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}